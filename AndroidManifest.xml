<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.bubba.yolandaslistoflists"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="16" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/listoflistslauncherx"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppBaseTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".OneListActivity" />
        <activity android:name="org.bubba.yolandaslistoflists.dragndrop.DragNDropListActivity" />
        <activity android:name=".EditKnownItemsActivity" />
    </application>

</manifest>